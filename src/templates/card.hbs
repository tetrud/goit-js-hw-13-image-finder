
{{#each this}}

  <li class="photo-card">
  <img class="image" src="{{webformatURL}}" alt="{{tags}}" data-source="{{largeImageURL}}"/>

  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      <span>{{likes}}</span>
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
       <span>{{views}}</span>
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
       <span>{{comments}}</span>
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
       <span>{{downloads}}</span>
    </p>
  </div>


</li>
{{/each}}

